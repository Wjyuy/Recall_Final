<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.boot.dao.AdminDAO">

	<select id="getAdminById" parameterType="string" resultType="com.boot.dto.AdminDTO">
		SELECT *
		FROM admin
		WHERE id = #{id}
	</select>

</mapper>